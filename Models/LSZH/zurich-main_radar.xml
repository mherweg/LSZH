<?xml version="1.0" encoding="iso-8859-1"?> 
<PropertyList>
  <path>zurich-main_radar.ac</path>
	
  <animation>
    <type>rotate</type>
    <object-name>Radar</object-name>
    <object-name>Radar2</object-name>
	<property>/sim/time/elapsed-sec</property>
    <factor>90.0</factor>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>-1</z>
    </axis>
  </animation>
	
  <animation>
    <type>range</type>
    <min-m>0</min-m>
    <max-m>5000</max-m>
  </animation>
</PropertyList>